{
  "name": "@cors.sh/service-layer",
  "version": "0.0.0",
  "description": "CORS.SH's service layer for managing api keys & subscription",
  "homepage": "https://cors.sh",
  "scripts": {
    "prisma:format": "npx prisma format",
    "prisma:studio": "npx prisma studio",
    "prisma:generate": "npx prisma generate",
    "prisma:generate:watch": "npx prisma generate --watch",
    "postinstall": "npm run prisma:generate && npm run prisma:format",
    "db:push": "npx prisma db push",
    "setup:prod": "echo 'passing'",
    "clean": "rimraf .build",
    "dev": "sls offline start --stage development",
    "deploy:staging": "yarn clean && yarn setup:staging && yarn db:push && sls deploy --stage staging",
    "deploy:prod": "yarn clean && yarn setup:prod && yarn db:push && sls deploy --stage production"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.79.0",
    "@aws-sdk/client-ses": "^3.163.0",
    "@aws-sdk/s3-request-presigner": "^3.88.0",
    "@prisma/client": "^4.8.0",
    "@vendia/serverless-express": "^4.8.0",
    "body-parser": "^1.20.0",
    "cors": "^2.8.5",
    "dayjs": "^1.11.5",
    "express": "^4.18.1",
    "express-useragent": "^1.0.15",
    "jsonwebtoken": "^8.5.1",
    "multer": "^1.4.4",
    "nanoid": "^3.3.3",
    "reading-time": "^1.5.0",
    "serverless-http": "^3.0.1",
    "stripe": "^10.7.0"
  },
  "devDependencies": {
    "@types/aws-lambda": "^8.10.95",
    "@types/body-parser": "^1.19.2",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "@types/jsonwebtoken": "^8.5.9",
    "@types/multer": "^1.4.7",
    "@types/node": "^17.0.29",
    "prisma": "^4.8.0",
    "rimraf": "^3.0.2",
    "serverless": "^3.17.0",
    "serverless-domain-manager": "^6.0.3",
    "serverless-offline": "^8.7.0",
    "serverless-webpack": "^5.7.1",
    "serverless-webpack-prisma": "^1.1.0",
    "ts-loader": "^9.3.0",
    "typescript": "^4.6.4",
    "webpack": "^5.72.1",
    "webpack-node-externals": "^3.0.0"
  }
}